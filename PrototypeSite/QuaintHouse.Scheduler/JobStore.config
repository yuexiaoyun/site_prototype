<?xml version="1.0" encoding="utf-8" ?>
<jobStore>
  <!-- Trigger type: 
  cron, 
  monthly : [dayOfMonth, hour, minute], 
  daily : [hour, minute], 
  hourly : [interval], 
  minutely : [interval], 
  secondly : [interval] 
  -->
  <jobs>
    <job name="sampleJob" enabled="true">
      <!-- ActionId -->
      <action>sampleAction</action>
      <!-- ClassType -->
      <type>Jobs.Sample, Jobs</type>
      <!-- Trigger -->
      <trigger type="daily" value="[10, 0]" />
      <!-- Params -->
      <params>
        <param name="" value="" />
      </params>
    </job>
  </jobs>
</jobStore>
